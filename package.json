{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dev",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" ",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dev/src/main",
    "lint": "eslint typings/**/* \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest-unit.json",
    "test:watch": "jest --config ./test/jest-unit.json --watch",
    "test:cov": "jest --config ./test/jest-unit.json --coverage",
    "test:integration": "cross-env IS_INTEGRATION_TESTING=true TZ=GMT/UTC jest --runInBand --bail --reporters default jest-stare --detectOpenHandles --forceExit --verbose",
    "test:integration:auth": "npm run test:integration -- auth.controller.spec.ts",
    "test:integration:app": "npm run test:integration -- app.controller.spec.ts",
    "test:integration:watch": "cross-env IS_RUNNING_TEST=true jest --detectOpenHandles --watchAll --verbose ",
    "test:integration:cov": "cross-env IS_RUNNING_TEST=true jest --detectOpenHandles  --coverage --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "nest g module $MODULE && nest g controller $MODULE",
    "migrate:latest": "knex migrate:latest --knexfile=./dev/knexfile.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.28.0",
    "@aws-sdk/s3-request-presigner": "^3.28.0",
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "biguint-format": "^1.0.2",
    "body-parser": "^1.19.0",
    "buffer": "^6.0.3",
    "bunyan": "^1.8.15",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connection-string": "^4.3.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-more": "^1.0.3",
    "express-http-context": "^1.2.4",
    "flake-idgen": "^1.4.0",
    "fs-extra": "^10.0.0",
    "hash.js": "^1.1.7",
    "helmet": "^4.6.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "mem": "^8.1.1",
    "nestjs-knex": "^2.0.0",
    "nodemailer": "^6.6.1",
    "on-headers": "^1.0.2",
    "p-queue": "^7.1.0",
    "p-retry": "^4.5.0",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-httpapikey": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "redis-cluster": "0.0.8",
    "redis-clustr": "^1.7.0",
    "redis-mock": "^0.56.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "sha256": "^0.2.0",
    "source-map-support": "^0.5.19",
    "sqlstring": "^2.3.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@prisma/client": "^3.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.6",
    "@types/cache-manager-redis-store": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/flake-idgen": "^0.1.30",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.29",
    "@types/nodemailer": "^6.4.2",
    "@types/on-headers": "^1.0.0",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^8.6.1",
    "@types/redis-mock": "^0.17.0",
    "@types/sqlstring": "^2.3.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/verror": "^1.10.4",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "jest-circus": "^27.0.6",
    "jest-stare": "^2.2.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prisma": "^3.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint -- --cache --fix"
    ],
    "*.js": "eslint --cache --fix"
  }
}
